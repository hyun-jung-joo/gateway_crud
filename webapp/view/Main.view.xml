<mvc:View
    controllerName="sync.c22.gateway.crud.controller.Main"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:t="sap.ui.table"
>
    <Page
        id="page"
        title="{i18n>title}"
    >
        <!-- 데이터 Read 구현 -->
        <t:Table
            rows="{/CarrierSet}"
            class="sapUiSmallMargin"
        >
            <t:extension>
                <Title text="{i18n>titleTable}" />
            </t:extension>
            <t:columns>
                <!-- Carrid -->
                <t:Column>
                    <t:label>
                        <Text text="{i18n>columnCarrid}" />
                    </t:label>
                    <t:template>
                        <Text text="{Carrid}" />
                    </t:template>
                </t:Column>

                <!-- Carrname -->
                <t:Column>
                    <t:label>
                        <Text text="{i18n>columnCarrname}" />
                    </t:label>
                    <t:template>
                        <Text text="{Carrname}" />
                    </t:template>
                </t:Column>

                <!-- Currcode -->
                <t:Column>
                    <t:label>
                        <Text text="{i18n>columnCurrcode}" />
                    </t:label>
                    <t:template>
                        <Text text="{Currcode}" />
                    </t:template>
                </t:Column>

                <!-- Url -->
                <t:Column>
                    <t:label>
                        <Text text="{i18n>columnUrl}" />
                    </t:label>
                    <t:template>
                        <Text text="{Url}" />
                    </t:template>
                </t:Column>
            </t:columns>
        </t:Table>
    </Page>
</mvc:View>
